                <td class="table-info head_15m">15m EMA<br>Cambio</td>
                <td class="table-danger head_1h">1h<br>Precio</td>
                <td class="table-danger head_1h">1h BB<br>Estado</td>
                <td class="table-danger head_1h">1h BB<br>Cambio</td>
                <td class="table-danger head_1h">1h EMA<br>Estado</td>
                <td class="table-danger head_1h">1h EMA<br>Cambio</td>
            </tr>
        </thead>
        <tbody>
            {{#each tikers}}
            <tr>
                <td>{{_id}}</td>
                <td>{{price}}</td>
                <td>&nbsp;</td>
                <td>{{perc_1m}}%</td>
