                <td>-</td>
                <td>-</td>
                <td>{{perc_5m}}%</td>
                <td>-</td>
                <td>-</td>
                <td>-</td>
                <td>-</td>
                <td>{{perc_15m}}%</td>
                <td>-</td>
                <td>-</td>
                <td>-</td>
                <td>-</td>
                <td>{{perc_1h}}</td>
                <td>-</td>
                <td>-</td>
                <td>-</td>
                <td>-</td>                
            </tr>
            {{else}}
                <td class="text-warning" colspan="23">
                    No se encontraron registros
                </td>
            {{/each}}
            
        </tbody >
    </table>
</div>