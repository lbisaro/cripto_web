<div class="container-fluid" id="variacion_del_precio">
    <table class="table table-sm table-striped">
        <caption>Variacion del precio</caption>
        <thead>
            <tr class="strong">
                <td>ID</td>
                <td>Precio actual</td>
                <td>% 1m</td>
                <td>% 3m</td>
                <td>% 5m</td>
                <td>% 15m</td>
                <td>% 1h</td>
            </tr>
        </thead>
        <tbody>
            {{#each tikers}}
            <tr>
                <td>{{_id}}</td>
                <td>{{price}}</td>
                <td>{{perc_1m}}</td>
                <td>{{perc_3m}}</td>
                <td>{{perc_5m}}</td>
                <td>{{perc_15m}}</td>
                <td>{{perc_1h}}</td>
            </tr>
            {{else}}
                <td class="text-warning" colspan="23">
                    No se encontraron registros
                </td>
            {{/each}}
            
        </tbody >
    </table>
</div>