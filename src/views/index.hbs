<div class="container-fluid">
    <table class="table table-sm table-striped">
        <thead>
            <tr class="strong">
                <td>ID</td>
                <td>Precio</td>
                <td>% MA200</td>
                <td class="table-success head_1m">1m<br>Precio</td>
                <td class="table-success head_1m">1m BB<br>Estado</td>
                <td class="table-success head_1m">1m BB<br>Cambio</td>
                <td class="table-success head_1m">1m EMA<br>Estado</td>
                <td class="table-success head_1m">1m EMA<br>Cambio</td>
                <td class="table-warning head_5m">5m<br>Precio</td>
                <td class="table-warning head_5m">5m BB<br>Estado</td>
                <td class="table-warning head_5m">5m BB<br>Cambio</td>
                <td class="table-warning head_5m">5m EMA<br>Estado</td>
                <td class="table-warning head_5m">5m EMA<br>Cambio</td>
